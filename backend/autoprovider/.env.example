# MySQL 配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=123456
DB_NAME=autoprovider_open
DB_CONNECTION_LIMIT=50
DB_QUEUE_LIMIT=100
DB_ACQUIRE_TIMEOUT=30000
DB_CONNECT_TIMEOUT=10000
DB_IDLE_TIMEOUT=60000
DB_WAIT_FOR_CONNECTIONS=true
DB_KEEP_ALIVE=true
DB_KEEP_ALIVE_DELAY=30000

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_FAMILY=4
REDIS_KEEP_ALIVE=true
REDIS_ENABLE_READY_CHECK=false
REDIS_LAZY_CONNECT=true
REDIS_RETRY_DELAY_ON_FAILOVER=100
REDIS_RETRY_DELAY_ON_CLUSTER_DOWN=300
REDIS_MAX_RETRIES_PER_REQUEST=3
REDIS_CONNECT_TIMEOUT=10000

# JWT 配置
JWT_SECRET=change_me_to_a_long_random_string
JWT_EXPIRES_IN=7d

# API 基础路径配置（前端）
VITE_API_BASE_URL=http://localhost:3000
VITE_API_VERSION=/api/v1

# Docker 连接配置（优先 DOCKER_HOST，其次 DOCKER_SOCKET_PATH）
# Windows 使用默认管道：//./pipe/docker_engine
# Linux/macOS 默认：/var/run/docker.sock
DOCKER_HOST=
DOCKER_PORT=
DOCKER_SOCKET_PATH=

# Dokploy（如需自动创建远程服务）
DOKPLOY_BASE_URL=
DOKPLOY_API_KEY=

# 项目存放目录（可选，未配置则使用内置默认路径）
PROJECTS_BASE_PATH=
